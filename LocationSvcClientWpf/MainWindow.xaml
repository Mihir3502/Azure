<Window x:Class="LocationSvcClientWpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LocationSvcClientWpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75" />
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width=".5*" />
            <ColumnDefinition Width=".5*" />
        </Grid.ColumnDefinitions>
        <Button x:Name="callServiceButton" Content="Call Service"  Grid.Row="0" Grid.Column="0" Margin="25" Click="callServiceButton_Click"></Button>
        <Button x:Name="signInButton" Content="Sign In"  Grid.Row="0" Grid.Column="1" Margin="25" Click="signInButton_Click"></Button>
        <TextBox x:Name="ServiceResult" HorizontalAlignment="Left" Height="23" Margin="106,91,0,0" Grid.Row="1" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
